```c
#include <stdio.h>
#include <stdlib.h>

// Structure for a node in the linked list
struct Node {
    int data;
    struct Node* next;
};

// Function to reverse a linked list in-place
void reverseList(struct Node** head) {
    struct Node* prev = NULL; // Pointer to the previous node
    struct Node* current = *head; // Pointer to the current node
    struct Node* next = NULL; // Pointer to the next node

    // Traverse the list and reverse the pointers
    while (current!= NULL) {
        next = current->next; // Store the next node
        current->next = prev; // Reverse the current node's pointer
        prev = current; // Move to the previous node
        current = next; // Move to the next node
    }

    // Update the head pointer to point to the new first node
    *head = prev;
}

// Function to print the reversed linked list
void printList(struct Node* head) {
    while (head!= NULL) {
        printf("%d -> ", head->data);
        head = head->next;
    }
    printf("null\n");
}

// Test the function
