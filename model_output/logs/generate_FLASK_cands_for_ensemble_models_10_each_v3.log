    height_pairs = list(height_dict.items())

    # Step 3: Sort the list of tuples by height in descending order
    sorted_pairs = sorted(height_pairs, key=lambda x: x[1], reverse=True)

    # Step 4: Reverse the list to get names in descending order by height
    sorted_names = [pair[0] for pair in sorted_pairs]

    return sorted_names

